{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Udacity advanced full-stack web developer training - Second Project - node_express_postgres",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "create-dev-db": "db-migrate --config database.json db:create storefront_dev && db-migrate up -e dev",
    "create-test-db": "db-migrate --config database.json db:create storefront_test",
    "dev-migrations": "db-migrate up -e dev",
    "reset-test-db": "db-migrate reset -e test",
    "start": "yarn build && node ./dist/server.js",
    "reset-dev-db": "db-migrate reset -e dev",
    "test": "db-migrate up -e test && yarn build && cross-env ENV=test jasmine --random=false && db-migrate reset -e test",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "tsc": "tsc",
    "prettier": "npx prettier --config .prettierrc --write src/**/*.ts",
    "seed":"node ./dist/SeedDB/SeedDB.js"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/jasmine": "^4.3.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "prettier": "^2.8.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsconfig": "^7.0.0"
  }
}
